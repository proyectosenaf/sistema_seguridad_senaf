    "dayjs": "^1.11.18",
    "docx": "^9.5.1",
    "dotenv": "16.4.5",
    "exceljs": "^4.4.0",
    "express": "^4.21.2",
    "express-async-errors": "3.1.1",
    "express-fileupload": "^1.5.2",
    "express-oauth2-jwt-bearer": "1.6.0",
    "express-rate-limit": "7.4.0",
    "express-validator": "^7.2.1",
    "helmet": "^7.2.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.2.0",
    "mongodb": "^6.20.0",
