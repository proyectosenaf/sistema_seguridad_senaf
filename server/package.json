    "start": "node src/server.js",
    "start:prod": "cross-env NODE_ENV=production node src/server.js",
    "lint": "node -e \"console.log('add eslint if desired')\""
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "celebrate": "^15.0.3",
    "compression": "^1.8.1",
