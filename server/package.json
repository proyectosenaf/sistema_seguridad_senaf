{
  "name": "senaf-api",
  "version": "1.0.1",
  "type": "module",
  "main": "src/server.js",
  "engines": {
    "node": ">=18.17"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development PORT=4000 API_PORT=4000 IAM_DEV_ALLOW_ALL=1 nodemon --ext js,mjs,cjs,json --signal SIGTERM src/server.js",
    "seed": "node src/scripts/seed.js",
